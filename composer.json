{
  "name": "inpsyde/multilingual-press",
  "description": "The multisite-based free open source plugin for your multilingual WordPress websites.",
  "type": "wordpress-plugin",
  "keywords": [
    "bilingual",
    "i18n",
    "international",
    "internationalization",
    "l10n",
    "lang",
    "language",
    "localization",
    "multi",
    "multilanguage",
    "multilingual",
    "multisite",
    "network",
    "translation",
    "wordpress",
    "wp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Inpsyde GmbH",
      "email": "hello@inpsyde.com",
      "homepage": "https://inpsyde.com",
      "role": "Company"
    }
  ],
  "support": {
    "issues": "https://github.com/inpsyde/MultilingualPress/issues",
    "forum": "https://wordpress.org/support/plugin/multilingualpress",
    "wiki": "https://github.com/inpsyde/MultilingualPress/wiki",
    "source": "https://github.com/inpsyde/MultilingualPress",
    "docs": "http://docs.multilingualpress.org"
  },
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "brain/monkey": "^1.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.2",
    "gmazzap/andrew": "~2.1",
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^3.0",
    "wimg/php-compatibility": "^8.0",
    "wp-coding-standards/wpcs": "^0.14"
  },
  "autoload": {
    "psr-4": {
      "Inpsyde\\MultilingualPress\\": "src"
    },
    "classmap": [
      "src/inc"
    ],
    "files": [
      "inc/functions.php",
      "inc/pluggable.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Inpsyde\\MultilingualPress\\Tests\\Unit\\": "tests/php/unit"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
